<h1><%= title %></h1>

<p> Welcome to <%= title %>. </p>

<% if (successMessage.length > 0) { %>
  <p class="success"> <%= successMessage %> </p>
<% } %>

<ul>
  <% if (userInfo) { %>
  <li> <a href="/logout">Log Out</a> </li>
  <li> <a href="/books">My Books</a> </li>
  <% } else { %>
    <li> <a href="/signup">Sign Up</a> </li>
    <li> <a href="/login">Log In</a> </li>
  <% } %>
</ul>
